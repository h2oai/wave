{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "target": "es6",
    "outDir": "./dist",
    "lib": [
      "dom",
      "es6",
    ],
    "types": [],
    "baseUrl": "./node_modules",
    "jsx": "preserve",
    "esModuleInterop": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}